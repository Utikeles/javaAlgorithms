public String needleInHaystack(String[][] haystack) 
{
    for (int i = 0; i < haystack.length;i++) {
        for (int j = 0; j < haystack[i].length;j++) {
            if ( haystack[i][j].equals("needle")) {
                return("row: "+i+", "+"column: "+j);
            }
        }
    }
    return "none";
}